body {
  background: #efe5d0 url(images/background.gif) top left;
  font-family: Verdana, Helvetica, Arial, sans-serif;


}

table {
  border: thin dotted #7e7e7e;
  padding: 10px;
  background-color: #e1ceb8;
  margin-bottom: 20px;

}
table, h1, h2{
  margin-left: auto;
  margin-right: auto;
}
h1, h2{
	text-align: center;
}
th {
  text-align: right;
  vertical-align: top;
  padding-right: 10px;
  padding-top: 2px;
}

td {
  vertical-align: top;
  padding-bottom: 15px;
}

table table {
  border: none;
  padding: 0px;
}

table table td {
  text-align: right;
  padding-bottom: 0px;
}
